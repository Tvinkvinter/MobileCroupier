<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="WrapContentDialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="windowMinWidthMajor">10%</item>
        <item name="windowMinWidthMinor">10%</item>
    </style>

    <style name="stroked_button">
        <item name="backgroundTint">@color/black</item>
        <item name="android:textColor">@drawable/base_button_csl</item>
        <item name="android:textSize">14sp</item>
        <item name="cornerRadius">30dp</item>
        <item name="strokeWidth">3dp</item>
        <item name="strokeColor">@drawable/base_button_csl</item>
        <item name="textAllCaps">true</item>
        <item name="android:layout_height">44dp</item>
    </style>
</resources>